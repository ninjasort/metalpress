{
  "name": "metalpress",
  "description": "Create a blog easily with Metalsmith.",
  "repository": {
    "type": "git",
    "url": "https://github.com/cameronroe/metalpress.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "scripts": {
    "lint": "eslint --ext .js,.jsx .",
    "lint:fix": "eslint --fix --ext .js,.jsx .",
    "test": "mocha --recursive --compilers js:babel-register",
    "test:watch": "npm t -- -w",
    "cover": "nyc npm t -r lcov",
    "check-coverage": "nyc check-coverage --lines 95 --functions 95 --branches 95",
    "report-coverage": "codecov -t 759de1b3-35e5-4742-a96e-3d6d37d6f4bd",
    "build": "babel src -d lib",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "dependencies": {
    "deep-assign": "2.0.0",
    "metalsmith": "2.1.0",
    "metalsmith-autoprefixer": "1.1.0",
    "metalsmith-broken-link-checker": "0.1.10",
    "metalsmith-build-date": "0.2.0",
    "metalsmith-collections": "0.7.0",
    "metalsmith-define": "2.0.1",
    "metalsmith-drafts": "0.0.1",
    "metalsmith-excerpts": "1.1.0",
    "metalsmith-fingerprint": "1.0.3",
    "metalsmith-firebase": "0.3.3",
    "metalsmith-html-minifier": "2.0.0",
    "metalsmith-ignore": "0.1.2",
    "metalsmith-imagemin": "1.2.0",
    "metalsmith-in-place": "1.4.3",
    "metalsmith-jekyll-dates": "0.0.5",
    "metalsmith-layouts": "1.6.4",
    "metalsmith-markdown": "0.2.1",
    "metalsmith-metadata": "0.0.4",
    "metalsmith-metallic": "0.3.1",
    "metalsmith-pagination": "1.4.0",
    "metalsmith-permalinks": "0.5.0",
    "metalsmith-rss": "1.0.0",
    "metalsmith-sass": "1.3.0",
    "metalsmith-sitemap": "1.0.0",
    "metalsmith-snippet": "2.0.0",
    "metalsmith-webpack": "1.0.3",
    "moment": "2.13.0",
    "tinyliquid": "0.2.31"
  },
  "devDependencies": {
    "assert-dir-equal": "1.0.1",
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-eslint": "6.0.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.8.0",
    "bower-webpack-plugin": "0.1.9",
    "chai": "3.5.0",
    "codecov": "1.0.1",
    "eslint": "2.9.0",
    "eslint-config-airbnb": "8.0.0",
    "eslint-plugin-import": "1.6.1",
    "mocha": "2.x",
    "nyc": "6.4.4",
    "rimraf": "2.5.2",
    "semantic-release": "^4.3.5",
    "sinon": "1.17.4"
  }
}
